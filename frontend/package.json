  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --build --noEmit",
    "lint:check": "eslint --cache --max-warnings 0 . && npm run lint:circular",
    "lint:fix": "eslint --cache --max-warnings 0 --fix .",
    "lint:circular": "madge --circular --extensions ts,tsx src/",
    "format:fix": "prettier --cache --write .",
    "format:check": "prettier --check .",
    "build": "npm run lint:check && tsc --build && vite build",
    "preview": "vite preview",
    "story": "storybook dev -p 6006",
    "story:build": "storybook build",
    "story:preview": "npx http-server ./storybook-static -p 6007 -c-1",
    "test": "vitest run",
    "test:unit": "vitest run --project unit",
    "test:unit:ui": "vitest --ui --project unit",
    "test:story": "vitest run --project storybook",
    "test:story:ui": "vitest --ui --project storybook"
  },
